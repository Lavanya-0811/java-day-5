public int countEvens(int input1,int input2,int input 3,int input4,int input5){
int cnt=0;
if(input<1=0)input1=(-1)*input1;
if(input<2=0)input1=(-1)*input2;
if(input<3=0)input1=(-1)*input3;
if(input<4=0)input1=(-1)*input4;
if(input<5=0)input1=(-1)*input5;
if(input1%2!=0)cnt++;
if(input2%2!=0)cnt++;
if(input3%2!=0)cnt++;
if(input4%2!=0)cnt++;
if(input5%2!=0)cnt++;
return cnt++;}
}
